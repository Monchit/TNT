<form id="formMakerAct" name="formMakerAct" class="form-horizontal well">
    <h4><u>FKK Action</u></h4>
    <div class="control-group">
        <label class="control-label" for="chb_process">Process :</label>
        <div class="controls">
            <label class="checkbox" for="chb_process-0">
                <input type="checkbox" name="chb_process" id="chb_process-0" value="Making Process">
                Making Process
            </label>
            <label class="checkbox" for="chb_process-1">
                <input type="checkbox" name="chb_process" id="chb_process-1" value="Return to Division for Trial">
                Return to Division for Trial
            </label>
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="nokpo">Complete Date** :</label>
        <div class="controls">
            <input type="text" id="comp_dt" name="comp_dt" class="span3" />
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="comment">FKK Comment :</label>
        <div class="controls">
            <textarea rows="3" id="fkkcomment" name="fkkcomment" class="span8"></textarea>
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="btnApv"></label>
        <div class="controls">
            <button id="btnApv" name="btnApv" type="button" class="btn btn-success"><i class="icon-ok icon-white"></i> Submit</button>
            @*<button id="btnRej" name="btnRej" type="button" class="btn btn-danger"><i class="icon-remove icon-white"></i> Reject</button>*@
        </div>
    </div>
</form>