
<form id="formDivAct" name="formDivAct" class="form-horizontal well">
    <h4><u>Division Action</u></h4>
    <div class="control-group">
        <label class="control-label" for="divorder">Division order No.** :</label>
        <div class="controls">
            <input type="text" id="divorder" name="divorder" class="span3" />
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="makername">Maker Name** :</label>
        <div class="controls">
            <input type="text" id="makername" name="makername" class="span3" />
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="comment">Division Comment :</label>
        <div class="controls">
            <textarea rows="3" id="divcomment" name="divcomment" class="span8"></textarea>
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="btnApv"></label>
        <div class="controls">
            <button id="btnApv" name="btnApv" type="button" class="btn btn-success"><i class="icon-ok icon-white"></i> Submit</button>
            <button id="btnRej" name="btnRej" type="button" class="btn btn-danger"><i class="icon-remove icon-white"></i> Reject</button>
        </div>
    </div>
</form>